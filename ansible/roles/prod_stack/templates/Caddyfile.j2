{
  servers {
    timeouts {
      read_header 15s
      read_body   15s
      write       15s
      idle        30s
    }
    max_header_size 8192   # oder 8KiB
  }
}

{{ domain }} {
  request_body {
    max_size 5MB
  }

  log {
    output discard
  }

  reverse_proxy http://query:8080
  tls dragan.gojic@student.ipso.ch
}
